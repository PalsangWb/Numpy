   "source": [
    "import turtle\n",
    "# Draw the square\n",
    "for i in range(4):\n",
    "    turtle.forward(100)\n",
    "    turtle.right(90)\n",
    "\n",
    "# Move to center and draw filled circle\n",
    "turtle.penup()\n",
    "turtle.goto(50, 50)\n",
    "turtle.pendown()\n",
    "turtle.fillcolor(\"red\")\n",
    "turtle.begin_fill()\n",
    "turtle.circle(80)\n",
    "turtle.end_fill()"
   ]
